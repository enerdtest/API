<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1 and TestNG report" verbose="1" thread-count="3" parallel="methods">
    <parameter name="a" value="4"></parameter>
    <parameter name="b" value="10"></parameter>
    <listeners>
        <listener class-name="utilities.testIReport"></listener>
        <listener class-name="utilities.iTestListener"></listener>
    </listeners>
    <test name="TestNG and Listener Report" >
        <groups>
            <run>
                <include name="oops"/>
            </run>
        </groups>
        <classes>
            <class name="testOOPs.TestOOPs"></class>
        </classes>
    </test>
</suite>

